    <owl:ObjectProperty rdf:about="&mdr;relatedToDataElementConceptRelationship">
        <owl:inverseOf rdf:resource="&mdr;relatingDataElementConceptRelationship"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
         <rdfs:range rdf:resource="&mdr;DataElementConceptRelationshipAssociation"/>
    </owl:ObjectProperty>
    
     <!-- &mdr;DataElement -->

     <owl:Class rdf:about="&mdr;DataElement">
        <rdfs:subClassOf rdf:resource="&mdr;AdministeredItem"/>
       	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;dataElementAdministrationRecord"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
        	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;representationClassQualifier"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
        	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;dataElementPrecision"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;representedByDataElementRepresentation"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
         <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;expressingDataElementConceptExpression"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
        	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;inputToDerivationInput"/>
