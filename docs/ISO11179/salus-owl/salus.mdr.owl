        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <!-- &mdr;AdministeredItem -->

    <owl:Class rdf:about="&mdr;AdministeredItem">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;administeredItemAdministrationRecord"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;administeredBy"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;submittedBy"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;registeredBy"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;having"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
		<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;describedBy"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- <owl:disjointWith rdf:resource="&mdr;AdministrationRecord"/> -->
    </owl:Class>

	<owl:ObjectProperty rdf:about="&mdr;administeredBy">
        <owl:inverseOf rdf:resource="&mdr;administeredItems"/>
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;StewardshipRelationship"/>
    </owl:ObjectProperty>    

    <owl:ObjectProperty rdf:about="&mdr;administeredItemAdministrationRecord">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;AdministrationRecord"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&mdr;classifiedBy">
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;ClassificationSchemeItem"/>
        <owl:inverseOf rdf:resource="&mdr;classifying"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&mdr;submittedBy">
		<owl:inverseOf rdf:resource="&mdr;submittedItems"/>
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;SubmissionRelationship"/>
    </owl:ObjectProperty>

	<owl:ObjectProperty rdf:about="&mdr;registeredBy">
		<owl:inverseOf rdf:resource="&mdr;registering"/>	
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;RegistrationAuthority"/>
    </owl:ObjectProperty>

	<owl:ObjectProperty rdf:about="&mdr;having">
		<owl:inverseOf rdf:resource="&mdr;grouping"/>
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;AdministeredItemContext"/>
    </owl:ObjectProperty>

	<owl:ObjectProperty rdf:about="&mdr;describedBy">
		<owl:inverseOf rdf:resource="&mdr;describing"/>	
        <rdfs:domain rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:range rdf:resource="&mdr;ReferenceDocument"/>
    </owl:ObjectProperty>
    
    <!-- &mdr;Property -->

    <owl:Class rdf:about="&mdr;Property">
        <rdfs:subClassOf rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;propertyAdministrationRecord"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
     <owl:ObjectProperty rdf:about="&mdr;propertyAdministrationRecord">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;Property"/>
        <rdfs:range rdf:resource="&mdr;AdministrationRecord"/>
		<rdfs:subPropertyOf rdf:resource="&mdr;administeredItemAdministrationRecord"/>
    </owl:ObjectProperty>
    
    <!-- &mdr;ObjectClass -->

    <owl:Class rdf:about="&mdr;ObjectClass">
        <rdfs:subClassOf rdf:resource="&mdr;AdministeredItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;objectClassAdministrationRecord"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- <owl:disjointWith rdf:resource="&mdr;Property"/> -->
    </owl:Class>
    
    <owl:ObjectProperty rdf:about="&mdr;objectClassAdministrationRecord">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;ObjectClass"/>
        <rdfs:range rdf:resource="&mdr;AdministrationRecord"/>
		<rdfs:subPropertyOf rdf:resource="&mdr;administeredItemAdministrationRecord"/>
	</owl:ObjectProperty>

    <!-- &mdr;DataElementConcept -->
    
    <owl:Class rdf:about="&mdr;DataElementConcept">
        <rdfs:subClassOf rdf:resource="&mdr;AdministeredItem"/>
    	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;dataElementConceptAdministrationRecord"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>  
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;havingDataElementConceptConceptualDomainRelationship"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>      
        <!-- <owl:disjointWith rdf:resource="&mdr;ObjectClass"/> -->
        <!-- <owl:disjointWith rdf:resource="&mdr;Property"/> -->
    </owl:Class>
    
    
    
    <owl:ObjectProperty rdf:about="&mdr;dataElementConceptAdministrationRecord">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
        <rdfs:range rdf:resource="&mdr;AdministrationRecord"/>
		<rdfs:subPropertyOf rdf:resource="&mdr;administeredItemAdministrationRecord"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&mdr;dataElementConceptObjectClass">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
         <rdfs:range rdf:resource="&mdr;ObjectClass"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&mdr;dataElementConceptProperty">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
        <rdfs:range rdf:resource="&mdr;Property"/>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;objectClassQualifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
     <owl:DatatypeProperty rdf:about="&mdr;propertyQualifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;propertyQualifier"/>
        <rdfs:range rdf:resource="&xsd;string"/>
     </owl:DatatypeProperty>
     
      <owl:ObjectProperty rdf:about="&mdr;expressedByDataElementConceptExpression">
        <owl:inverseOf rdf:resource="&mdr;expressingDataElementConceptExpression"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
         <rdfs:range rdf:resource="&mdr;DataElement"/>
    </owl:ObjectProperty>
    
     <owl:ObjectProperty rdf:about="&mdr;havingDataElementConceptConceptualDomainRelationship">
        <owl:inverseOf rdf:resource="&mdr;specifyingDataElementConceptConceptualDomainRelationship"/>
        <rdfs:domain rdf:resource="&mdr;DataElementConcept"/>
