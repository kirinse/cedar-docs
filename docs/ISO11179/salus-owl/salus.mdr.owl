    <owl:ObjectProperty rdf:about="&mdr;itemRegistrationAuthorityIdentifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;ItemIdentifier"/>
        <rdfs:range rdf:resource="&mdr;RegistrationAuthorityIdentifier"/>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;dataIdentifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;ItemIdentifier"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;version">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;ItemIdentifier"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <!-- &mdr;Contact -->
    
    <owl:Class rdf:about="&mdr;Contact">
