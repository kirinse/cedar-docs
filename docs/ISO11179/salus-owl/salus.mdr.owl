        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;effectiveDate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;untilDate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;changeDescription">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
        
    <owl:DatatypeProperty rdf:about="&mdr;administrativeNote">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
