    <owl:DatatypeProperty rdf:about="&mdr;registrationStatus">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&mdr;administrativeStatus">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&mdr;AdministrationRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
