            </owl:Restriction>
        </rdfs:subClassOf> 
        	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;derivedFromDerivationOutput"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf> 
        	<rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;exemplifiedByExemplification"/>
