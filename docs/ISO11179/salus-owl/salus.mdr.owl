        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;version"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&mdr;dataIdentifier"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- <owl:disjointWith rdf:resource="&mdr;RegistrationAuthorityIdentifier"/> -->
